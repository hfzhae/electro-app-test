{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "packager": "electron-packager . ebClient --out build --asar --overwrite --platform=win32 --arch=x64 --icon=./AppSystem.ico",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.7"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3"
  },
  "build": {
    "appId": "com.zydsoft.ebclient",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "./AppSystem.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./AppSystem.ico",
      "uninstallerIcon": "./AppSystem.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
